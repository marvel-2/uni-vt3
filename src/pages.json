{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^u--(.*)": "uview-plus/components/u-$1/u-$1.vue",
			"^up-(.*)": "uview-plus/components/u-$1/u-$1.vue",
			"^u-([^-].*)": "uview-plus/components/u-$1/u-$1.vue",
			// uni-ui
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		},
		{
			"path": "pages/my/my",
			"style": {
				"navigationBarTitleText": "我的"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#8a8a8a", // 设置tab-bar未被选中时的字体颜色
		"selectedColor": "#1296db", // 设置tab-bar 被选中时的字体颜色
		"backgroundColor": "#ffffff", // 设置tab-bar 背景颜色
		"list": [
			{
				"pagePath": "pages/index/index", // 设置页面跳转路径
				"text": "首页", // 设置tab-bar文字显示
				"iconPath": "/static/tabbar/_index.png", // 设置未被选中时的图标
				"selectedIconPath": "/static/tabbar/index.png" // 设置选中时的图标
			},{
				"pagePath": "pages/my/my",
				"text": "我的",
				"iconPath": "/static/tabbar/_my.png",
				"selectedIconPath": "/static/tabbar/my.png"
			}
		]
	}
}
